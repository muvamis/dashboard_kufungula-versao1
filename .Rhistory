# Função para mapear valores para bolinhas coloridas ou X
map_values <- function(value) {
if (is.na(value)) {
return("X")
} else if (value == "Sim") {
return('<span style="color:green;">•</span>') # Bolinha verde
} else if (value == "Nao") {
return('<span style="color:red;">•</span>')   # Bolinha vermelha
} else {
return(value)
}
}
